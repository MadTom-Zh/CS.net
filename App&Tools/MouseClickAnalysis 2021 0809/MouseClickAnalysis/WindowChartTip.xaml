<Window x:Class="MouseClickAnalysis.WindowChartTip"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WindowChartTip" Height="169" Width="339" WindowStartupLocation="CenterOwner">
    <Grid>
        <Rectangle Fill="Blue"  HorizontalAlignment="Left" Height="25" Margin="10,10,0,0"  VerticalAlignment="Top" Width="15"/>
        <Rectangle Fill="Green"  HorizontalAlignment="Left" Height="25" Margin="10,40,0,0"  VerticalAlignment="Top" Width="15"/>
        <Rectangle Fill="Red"  HorizontalAlignment="Left" Height="25" Margin="10,70,0,0"  VerticalAlignment="Top" Width="15"/>
        <Label Content="出现次数极多，每5次计数为1个高度单位；" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="30,11,0,0"/>
        <Label Content="出现次数一般，每1次计数为1个高度单位；" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="30,41,0,0"/>
        <Label Content="出现次数极少，每1次计数为5个高度单位；" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="30,71,0,0"/>

    </Grid>
</Window>
